<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ManagerMapper">
	<!-- 관리자 로그인 -->
	<select id="login" resultType="ManagerVo">
	SELECT * FROM MANAGER
	WHERE ID =#{id} AND PWD =#{pwd}
	</select>
	
</mapper>